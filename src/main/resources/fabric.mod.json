{
  "schemaVersion": 1,
  "id": "keystone",
  "version": "${version}",
  "name": "Keystone",
  "description": "Adds large scale world editing tools similar to the outdated program MCEdit.",
  "authors": ["CodeCracked"],
  "contact": {
    "homepage": "https://github.com/KeystoneTeam/KeystoneEditor",
    "issues": "https://github.com/KeystoneTeam/KeystoneEditor/issues",
    "sources": "https://github.com/KeystoneTeam/KeystoneEditor"
  },
  "license": "GPL-3.0",
  "icon": "assets/keystone/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "keystone.core.KeystoneMod"
    ],
    "main": [
      "keystone.core.KeystoneMod"
    ],
    "modmenu": [
      "keystone.compat.modmenu.KeystoneModMenu"
    ]
  },
  "mixins": [
    "keystone.mixins.common.json",
    "keystone.mixins.client.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": "${target_minecraft_versions}"
  }
}
